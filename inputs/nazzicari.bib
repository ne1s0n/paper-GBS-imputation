@article{lu_switchgrass_2013,
	title = {Switchgrass genomic diversity, ploidy, and evolution: novel insights from a network-based snp discovery protocol},
	volume = {9},
	shorttitle = {Switchgrass {Genomic} {Diversity}, {Ploidy}, and {Evolution}},
	url = {http://dx.doi.org/10.1371/journal.pgen.1003215},
	doi = {10.1371/journal.pgen.1003215},
	abstract = {Author SummaryRecent advances in sequencing technologies have enabled large-scale surveys of genetic diversity in model species with a wholly or partly sequenced reference genome. However, thousands of key species, which are essential for food, health, energy, and ecology, do not have reference genomes. To accelerate their breeding cycle via marker assisted selection, high-throughput genotyping is required for these valuable species, in spite of the absence of reference genomes. Based on genotyping by sequencing (GBS) technology, we developed a new single nucleotide polymorphism (SNP) discovery protocol, the Universal Network-Enabled Analysis Kit (UNEAK), which can be widely used in any species, regardless of genome complexity or the availability of a reference genome. Here we test this protocol on switchgrass, currently the prime energy crop species in the United States of America. In addition to the discovery of over a million SNPs and construction of high-density linkage maps, we provide novel insights into the genome complexity, ploidy, phylogeny, and evolution of switchgrass. This is only the beginning: we believe UNEAK offers the key to the exploration and exploitation of the genetic diversity of thousands of non-model species.},
	number = {1},
	urldate = {2015-03-11},
	journal = {PLoS Genet},
	author = {Lu, Fei and Lipka, Alexander E. and Glaubitz, Jeff and Elshire, Rob and Cherney, Jerome H. and Casler, Michael D. and Buckler, Edward S. and Costich, Denise E.},
	month = jan,
	year = {2013},
	pages = {e1003215},
	file = {PLoS Full Text PDF:/home/nelson/.mozilla/firefox/86tjhmc8.default/zotero/storage/34HSVETD/Lu et al. - 2013 - Switchgrass Genomic Diversity, Ploidy, and Evoluti.pdf:application/pdf}
}

@article{sonah_improved_2013,
	title = {An improved genotyping by sequencing ({GBS}) approach offering increased versatility and efficiency of snp discovery and genotyping},
	volume = {8},
	url = {http://dx.doi.org/10.1371/journal.pone.0054603},
	doi = {10.1371/journal.pone.0054603},
	abstract = {Highly parallel SNP genotyping platforms have been developed for some important crop species, but these platforms typically carry a high cost per sample for first-time or small-scale users. In contrast, recently developed genotyping by sequencing (GBS) approaches offer a highly cost effective alternative for simultaneous SNP discovery and genotyping. In the present investigation, we have explored the use of GBS in soybean. In addition to developing a novel analysis pipeline to call SNPs and indels from the resulting sequence reads, we have devised a modified library preparation protocol to alter the degree of complexity reduction. We used a set of eight diverse soybean genotypes to conduct a pilot scale test of the protocol and pipeline. Using ApeKI for GBS library preparation and sequencing on an Illumina GAIIx machine, we obtained 5.5 M reads and these were processed using our pipeline. A total of 10,120 high quality SNPs were obtained and the distribution of these SNPs mirrored closely the distribution of gene-rich regions in the soybean genome. A total of 39.5\% of the SNPs were present in genic regions and 52.5\% of these were located in the coding sequence. Validation of over 400 genotypes at a set of randomly selected SNPs using Sanger sequencing showed a 98\% success rate. We then explored the use of selective primers to achieve a greater complexity reduction during GBS library preparation. The number of SNP calls could be increased by almost 40\% and their depth of coverage was more than doubled, thus opening the door to an increase in the throughput and a significant decrease in the per sample cost. The approach to obtain high quality SNPs developed here will be helpful for marker assisted genomics as well as assessment of available genetic resources for effective utilisation in a wide number of species.},
	number = {1},
	urldate = {2015-03-23},
	journal = {PLoS ONE},
	author = {Sonah, Humira and Bastien, Maxime and Iquira, Elmer and Tardivel, Aurélie and Légaré, Gaétan and Boyle, Brian and Normandeau, Éric and Laroche, Jérôme and Larose, Stéphane and Jean, Martine and Belzile, François},
	month = jan,
	year = {2013},
	pages = {e54603},
	file = {PLoS Full Text PDF:/home/nelson/.mozilla/firefox/86tjhmc8.default/zotero/storage/94GBVRP5/Sonah et al. - 2013 - An Improved Genotyping by Sequencing (GBS) Approac.pdf:application/pdf}
}

