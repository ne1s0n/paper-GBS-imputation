	keywords = {Bayesian LASSO, Biotechnology, Genomic selection, plant breeding, Plant Genetics \& Genomics, Plant Pathology, Plant Physiology, Plant Sciences, Random Forest regression, Ridge regression, Triticum aestivum L.},
	pages = {1843--1852},
